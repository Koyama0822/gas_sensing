<launch>
  <!-- launch gas_patrol node -->
  <node pkg="turtlebot3_explore" type="gas_patrol.py" name="gas_patrol" output="screen">
    <rosparam command="load" file="$(find turtlebot3_explore)/param/turtlebot3_relay_points.yaml"/>
  </node>

  <node pkg="turtlebot3_explore" type="gas_mapping.py" name="gas_mapping">
      <rosparam>
      gas_visual_map_resolution: 0.2
      gas_visual_map_origin: [-5.0, -5.0, 0]
      gas_visual_scale: 1.0
      gas_visual_offset: 10.0
    </rosparam>
  </node>

</launch>
